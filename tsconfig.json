{
    "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "nodenext",
      "strict": true,
      "noImplicitAny": true,
      "allowJs": true,
      "checkJs": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "jsx": "react",
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "build"]
}
  